<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>hello</servlet-name>
        <servlet-class>MyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>hello2</servlet-name>
        <servlet-class>MyServlet2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>hello3</servlet-name>
        <servlet-class>MyServlet3</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>hello4</servlet-name>
        <servlet-class>MyServlet4</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>hellofin</servlet-name>
        <servlet-class>MyServletfin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>hello</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>hello2</servlet-name>
        <url-pattern>/hello2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>hello3</servlet-name>
        <url-pattern>/hello3</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>hello4</servlet-name>
        <url-pattern>/hello4</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>hellofin</servlet-name>
        <url-pattern>/hellofin</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>firstpage.jsp</welcome-file>
    </welcome-file-list>
</web-app>
